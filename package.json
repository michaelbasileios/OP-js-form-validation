{
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.2.0",
    "html-webpack-plugin": "^5.6.3",
    "prettier": "3.5.3",
    "style-loader": "^4.0.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.1"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack serve --config webpack.dev.js",
    "ghp-step0": "git branch gh-pages",
    "ghp-step1": "git checkout gh-pages && git merge main --no-edit",
    "ghp-step2": "git add dist -f && git commit -m \"Deployment commit\"",
    "ghp-deploy": "git subtree push --prefix dist origin gh-pages",
    "prettier write": "npx prettier . --write",
    "prettier check": "npx prettier . --check"
  }
}
